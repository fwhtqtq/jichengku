void shuru_jintaishuzu()
{
	//
	int size;
    printf("请输入静态数组的大小：");
    scanf("%d", &size);
    int array[size];
	//
	printf("请输入静态数组的元素：");
    for (int i = 0; i < size; i++) 
	{
        scanf("%d", &array[i]);	
	}
	//
	printf("静态数组的元素为：");
    for (int i = 0; i < size; i++) 
	{
        printf("%d ", array[i]);
    }
    printf("\n");
}
